<!--begin::Content-->
<div id="kt_app_content" data-user-id=<%=userId %> class="app-content flex-column-fluid">
    <!--begin::Content container-->
    <div id="kt_app_content_container" class="app-container container-xxl">
        <!--begin::Layout-->
        <div class="d-flex flex-column flex-lg-row">
            <!--begin::Sidebar-->
            <div class="flex-column flex-lg-row-auto w-lg-250px w-xl-350px mb-10">
                <!--begin::Card-->
                <div class="card mb-5 mb-xl-8">
                    <!--begin::Card body-->
                    <div class="card-body">
                        <!--begin::Summary-->
                        <!--begin::User Info-->
                        <div class="d-flex flex-center flex-column py-5">
                            <!--begin::Avatar-->
                            <!--begin::Avatar-->
                            <div class="card card-flush py-4">
                                <div class="card-body text-center pt-0">
                                    <div class="mb-3 w-150px h-150px rounded shadow-sm mx-auto">
                                        <div id="imagePreview" class="w-150px h-150px rounded shadow-sm mx-auto">
                                            <img class="user_img" style="height: 150px; width: 150px;" src="" alt="">
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <!--end::Avatar-->
                            <!--begin::Name-->
                            <a href="#" class="fs-3 text-gray-800 text-hover-primary fw-bold mb-3 username">Emma
                                Smith</a>
                            <!--end::Name-->
                            <!--begin::Position-->
                            <div class="mb-9">
                                <!--begin::Badge-->
                                <div class="badge badge-lg badge-light-primary d-inline role">Admin</div>
                                <!--begin::Badge-->
                            </div>
                            <!--end::Position-->
                        </div>
                        <!--end::User Info-->
                        <!--end::Summary-->
                        <div class="separator"></div>
                        <!--begin::Details content-->
                        <div id="kt_user_view_details" class="collapse show">
                            <div class="pb-5 fs-6">
                                <!--begin::Details item-->
                                <div class="fw-bold mt-5">Email</div>
                                <div class="text-gray-600">
                                    <a href="#" class="text-gray-600 text-hover-primary email">info@keenthemes.com</a>
                                </div>
                                <!--begin::Details item-->
                                <!--begin::Details item-->
                                <div class="fw-bold mt-5">Address</div>
                                <div class="text-gray-600 address">101 Collin Street,
                                    <br />Melbourne 3000 VIC
                                    <br />Australia
                                </div>
                                <!--begin::Details item-->
                                <!--begin::Details item-->
                                <div class="fw-bold mt-5">Language</div>
                                <div class="text-gray-600 language">English</div>
                                <!--begin::Details item-->
                            </div>
                        </div>
                        <!--end::Details content-->
                    </div>
                    <!--end::Card body-->
                </div>
                <!--end::Card-->
            </div>
            <!--end::Sidebar-->
            <!--begin::Content-->
            <div class="flex-lg-row-fluid ms-lg-15">
                <!--begin:::Tabs-->
                <ul class="nav nav-custom nav-tabs nav-line-tabs nav-line-tabs-2x border-0 fs-4 fw-semibold mb-8">
                    <!-- Overview Tab -->
                    <li class="nav-item">
                        <a class="nav-link text-active-primary pb-4 active" data-bs-toggle="tab"
                            href="#kt_user_view_overview_tab">Overview</a>
                    </li>

                    <!-- Inquiry Details Tab -->
                    <li class="nav-item" style="display: none;">
                        <a class="nav-link text-active-primary pb-4" data-kt-countup-tabs="true" data-bs-toggle="tab"
                            href="#kt_user_view_overview_security">Inquiry Details</a>
                    </li>

                    <!-- Properties Tab (Initially hidden) -->
                    <li class="nav-item" id="properties-tab" style="display: none;">
                        <a class="nav-link text-active-primary pb-4" data-bs-toggle="tab"
                            href="#kt_user_view_properties_tab">Properties</a>
                    </li>
                </ul>
                <!--end:::Tabs-->
                <!--begin:::Tab content-->
                <div class="tab-content" id="myTabContent">
                    <!--begin:::Overview Tab-->
                    <div class="tab-pane fade show active" id="kt_user_view_overview_tab" role="tabpanel">
                        <div class="card card-flush mb-6 mb-xl-9">
                            <div class="card-header mt-6">
                                <div class="card-title flex-column w-100" style="border-bottom: 1px solid #a4a4a4;">
                                    <h2 class="mb-1" style="color: #565656;">User's Details</h2>
                                    <div class="fs-6 fw-semibold text-muted"></div>
                                </div>
                            </div>
                            <div class="card-body p-9 pt-4">
                                <div class="table-responsive">
                                    <table class="table align-middle table-row-dashed gy-5"
                                        id="kt_table_users_login_session">
                                        <tbody class="fs-6 fw-semibold text-gray-600">
                                            <tr>
                                                <td>Username</td>
                                                <td class="username">Abc</td>
                                            </tr>
                                            <tr>
                                                <td>Email</td>
                                                <td class="email">smith@kpmg.com</td>
                                            </tr>
                                            <tr>
                                                <td>Is Active?</td>
                                                <td class="isAcitve">YES</td>
                                            </tr>
                                            <tr>
                                                <td>Role</td>
                                                <td class="role">Administrator</td>
                                            </tr>
                                            <tr>
                                                <td>Phone</td>
                                                <td class="phone">9876543210</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>

                    <!--begin:::Inquiry Details Tab-->
                    <div class="tab-pane fade" id="kt_user_view_overview_security" role="tabpanel">
                        <div class="card pt-4 mb-6 mb-xl-9">
                            <div class="card-header border-0">
                                <div class="card-title w-100" style="border-bottom: 1px solid #a4a4a4;">
                                    <h2 class="mb-1">Inquiry Details</h2>
                                </div>
                            </div>
                            <div class="card-body pt-0 pb-5">
                                <div class="table-responsive">
                                    <table class="table table-row-dashed align-middle gs-0 gy-4">
                                        <thead>
                                            <tr class="fw-bold text-muted">
                                                <th class="min-w-200px">Property Name</th>
                                                <th class="min-w-300px">Message</th>
                                            </tr>
                                        </thead>
                                        <tbody id="inquiry-details-body">
                                            <tr>
                                                <td class="text-truncate mw-100px inquiry_property"
                                                    title="3BHK Luxury Villa in LA">3BHK Luxury Villa in LA</td>
                                                <td class="question">Is this still available?</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>

                    <!--begin:::Properties Tab (Hidden by Default, Show if Agent)-->
                    <div class="tab-pane fade" id="kt_user_view_properties_tab" role="tabpanel">
                        <div class="card pt-4 mb-6 mb-xl-9">
                            <div class="card-header border-0">
                                <div class="card-title w-100" style="border-bottom: 1px solid #a4a4a4;">
                                    <h2 class="mb-1" style="color: #565656;">Agent's Properties</h2>
                                </div>
                            </div>
                            <div class="card-body pt-0 pb-5">
                                <div class="table-responsive">
                                    <table class="table table-row-dashed align-middle gs-0 gy-4">
                                        <thead>
                                            <tr class="fw-bold text-muted">
                                                <th class="min-w-150px">Title</th>
                                                <th class="min-w-80px">Type</th>
                                                <th class="min-w-100px">City</th>
                                                <th class="min-w-120px">Price</th>
                                                <th class="min-w-80px">For</th>
                                            </tr>
                                        </thead>
                                        <tbody id="properties-body">
                                            <tr>
                                                <td>Luxury Villa in Miami</td>
                                                <td>Villa</td>
                                                <td>Miami</td>
                                                <td>₹2,50,00,000</td>
                                                <td>Sale</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!--end:::Tab content-->
            </div>
            <!--end::Content-->
        </div>
        <!--end::Layout-->
        <!--end::Modals-->
    </div>
